			var panel = new actuate.viewer.BrowserPanel();
			uiCfg = new actuate.viewer.UIConfig();
			uiCfg.setContentPanel(panel);
			var vwr = new actuate.Viewer("acViewer", uiCfg);
			var uiOptions = new actuate.viewer.UIOptions( );
			uiOptions.enableToolBar(false);
			vwr.setUIOptions(uiOptions);
			vwr.setWidth($(window).width()/1.75);
			vwr.setHeight(850);
			vwr.setReportName(rpt);
			vwr.setParameters({'pDocID': <VALUE-OF>vars["docToLoadFirst"]</VALUE-OF>, 'pHideBanner':true});
			vwr.submit();
			
function showDocDetail(docid) {
			vwr.setParameters({'pDocID': docid, 'pHideBanner':true});
			vwr.submit();
			$("#acViewer").show();
}

</script>
